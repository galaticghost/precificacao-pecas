class HorasTrabalhadas{
    -id: int
    -numeroHoras: int
    -salarioDesejado: double
    -valorHora: double
    -valorTotalHoras: double
    --
    +getters()
    +setters()
    +calcular(salarioDesejado,numeroHoras)
    +inserir()
}

