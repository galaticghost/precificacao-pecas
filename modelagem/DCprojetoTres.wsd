class Venda {
    -id: int
    -idCustosVariaveis: int
    -idHorasTrabalhadas: int
    -idCustosFixos: int
    -totalParcial: double
    -margemLucro: int
    -valorLucro: int
    -despesaComercializacao: int
    -valorDespesa: double
    -valorTotal: double
    --
    +getters()
    +setters()
    +consultarTotalParcial()
    +calcularLucro()
    +calcularDespesa()
    +calcularTotalVenda()
    +inserir()
}

